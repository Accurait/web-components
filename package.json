{
  "name": "@flnt-accurait/web-components",
  "version": "0.0.9",
  "type": "module",
  "main": "./nuxt.config.ts",
  "private": false,
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "storybook": "concurrently 'pnpm:watch:*'",
    "watch:tailwind": "npx tailwindcss -i ./assets/css/tailwind-input.css -o ./assets/css/tailwind-output.css --watch",
    "watch:storybook": "storybook dev -p 6006",
    "build:tailwind": "npx tailwindcss -i ./assets/css/tailwind-input.css -o ./assets/css/tailwind-output.css",
    "build-storybook": "pnpm build:tailwind && storybook build",
    "release": "pnpm version patch && pnpm publish --access public"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.6.6",
    "@storybook/addon-essentials": "7.0.5",
    "@storybook/addon-interactions": "7.0.5",
    "@storybook/addon-links": "7.0.5",
    "@storybook/blocks": "7.0.5",
    "@storybook/testing-library": "0.0.14-next.2",
    "@storybook/vue3": "7.0.5",
    "@storybook/vue3-vite": "7.0.5",
    "concurrently": "^8.0.1",
    "nuxt": "^3.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.0.5",
    "storybook-tailwind-dark-mode": "^1.0.22"
  },
  "dependencies": {
    "class-variance-authority": "^0.5.2",
    "flowbite": "^1.6.5",
    "tailwind-merge": "^1.12.0"
  }
}
